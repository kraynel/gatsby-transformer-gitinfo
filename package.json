{
  "name": "gatsby-transformer-gitinfo",
  "description": "Gatsby transformer plugin which add git info to File nodes created by gatsby-source-filesystem",
  "version": "1.1.0",
  "author": "Kevin Raynel",
  "bugs": {
    "url": "https://github.com/kraynel/gatsby-transformer-gitinfo/issues"
  },
  "dependencies": {
    "@babel/runtime": "7.7.2",
    "simple-git": "1.126.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "babel-preset-gatsby-package": "0.2.9",
    "cross-env": "5.2.1",
    "jest": "24.9.0"
  },
  "homepage": "https://github.com/kraynel/gatsby-transformer-gitinfo",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "git"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kraynel/gatsby-transformer-gitinfo.git"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "test": "jest"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
